{
    "name": "Onboarding page",
    "description": "Display onboarding and uninstall pages",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "./popup.html"
    },
    "background":{
        "service_worker": "./background.js"
    },
    "permissions": ["storage"],
    "commands":{
        "_execute_action":{
            "suggested_key":{
                "chromeos": "Alt+Shift+O",
                "windows": "Alt+Shift+O"
            }
        }
    }
}
